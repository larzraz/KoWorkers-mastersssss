<Window x:Class="KoWorkerGui.AddShift_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KoWorkerGui"
        mc:Ignorable="d"
        Title="AddShift_Window" Height="300" Width="300">
    <Grid>
        <Button x:Name="sixToTen_Buttn" Content="6 - 10" HorizontalAlignment="Left" Height="27" Margin="10,10,0,0" VerticalAlignment="Top" Width="67" Click="sixToTen_Buttn_Click"/>
        <Button x:Name="seventeenTo00_button" Content="17 - 00" HorizontalAlignment="Left" Height="27" Margin="10,42,0,0" VerticalAlignment="Top" Width="67" Click="seventeenTo00_button_Click"/>
        <Button x:Name="elevenTo14_Buttn_Copy1" Content="11 - 14" HorizontalAlignment="Left" Height="27" Margin="10,74,0,0" VerticalAlignment="Top" Width="67" Click="elevenTo14_Buttn_Copy1_Click"/>
        <Button x:Name="Okay_Button" Content="Ok" HorizontalAlignment="Left" Height="24" Margin="116,116,0,0" VerticalAlignment="Top" Width="77" Click="Okay_Button_Click"/>

    </Grid>
</Window>
