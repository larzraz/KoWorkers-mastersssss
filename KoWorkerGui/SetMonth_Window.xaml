<Window x:Class="KoWorkerGui.SetMonth_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KoWorkerGui"
        mc:Ignorable="d"
        Title="SetMonth_Window" Height="100" Width="200">
    <Grid>
        <ComboBox x:Name="Year_Button" HorizontalAlignment="Left" Height="20" Margin="10,39,0,0" VerticalAlignment="Top" Width="47"/>
        <ComboBox x:Name="Month_Button" HorizontalAlignment="Left" Height="20" Margin="62,39,0,0" VerticalAlignment="Top" Width="89"/>
        <Label x:Name="Year_Label" Content="År" HorizontalAlignment="Left" Height="24" Margin="10,10,0,0" VerticalAlignment="Top" Width="47"/>
        <Label x:Name="Month_Label" Content="Måned" HorizontalAlignment="Left" Height="24" Margin="62,10,0,0" VerticalAlignment="Top" Width="47"/>
        <Button x:Name="SetMonthOK_Button" Content="OK" HorizontalAlignment="Left" Height="21" Margin="156,39,0,0" VerticalAlignment="Top" Width="28" Click="SetMonthOK_Button_Click"/>

    </Grid>
</Window>
