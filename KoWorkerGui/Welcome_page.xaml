<Page x:Class="KoWorkerGui.Welcome_page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:KoWorkerGui"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="600"
      Title="Welcome_page">

    <Grid>
        <Label x:Name="Welcome_Label" Content="Velkommen til KoWorkers" HorizontalAlignment="Left" Height="24" Margin="169,10,0,0" VerticalAlignment="Top" Width="274"/>
        <Button x:Name="Admin_Button" Content="Medarbejderadministration" HorizontalAlignment="Left" Height="38" Margin="145,75,0,0" VerticalAlignment="Top" Width="183" BorderBrush="#FFD6CFCF" Background="White" Click="Admin_Button_Click"/>
        <Button x:Name="TjekInd_Ud_Button" Content="Tjek In / Ud" HorizontalAlignment="Left" Height="38" Margin="145,118,0,0" VerticalAlignment="Top" Width="183" BorderBrush="#FFD6CFCF" Background="White" Click="TjekInd_Ud_Button_Click" />
        <Button x:Name="TimeSchedule_Button" Content="Arbejdsplan" HorizontalAlignment="Left" Height="38" Margin="145,161,0,0" VerticalAlignment="Top" Width="183" BorderBrush="#FFD6CFCF" Background="White" Click="TimeSchedule_Button_Click" />

    </Grid>
</Page>
